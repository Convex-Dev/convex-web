{
  "name": "convex-web",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "server": "npx shadow-cljs server",
    "app:watch": "bb mtempl :dev '[:module/app :ext/shadow-cljs]' -- shadow-cljs -A__ watch :module/app",
    "app:release": "bb mtempl '[:module/app :ext/shadow-cljs]' -- shadow-cljs -A__ release :module/app",
    "styles:watch": "TAILWIND_MODE=watch npx postcss tailwind.css -o src/main/resources/public/css/styles.css --verbose -w",
    "styles:release": "NODE_ENV=production npx postcss tailwind.css -o src/main/resources/public/css/styles.css --verbose"
  },
  "dependencies": {
    "@headlessui/react": "^1.4.1",
    "@heroicons/react": "^1.0.5",
    "@tailwindcss/typography": "^0.4.1",
    "@tailwindcss/ui": "^0.7.2",
    "@types/react": "^17.0.33",
    "codemirror": "^5.63.3",
    "highlight.js": "^11.2.0",
    "jdenticon": "^3.1.1",
    "qrcode.react": "^1.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-highlight.js": "^1.0.7",
    "react-markdown": "^5.0.3",
    "react-resizable": "^3.0.4",
    "react-tippy": "^1.4.0",
    "timeago.js": "^4.0.2"
  },
  "devDependencies": {
    "autoprefixer": "^10.3.3",
    "postcss": "^8.3.6",
    "postcss-cli": "^8.3.1",
    "shadow-cljs": "^2.15.10",
    "tailwindcss": "^2.2.7"
  }
}
